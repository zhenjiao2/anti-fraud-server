{
  "azure_ai_inference": {
    "endpoint": "{{env.endpoint}}",
    "api_key": "{{env.api_key}}",
    "model_name": "gpt-4.1",
    "temperature": 0.3,
    "top_p": 1.0,
    "max_tokens": 1000,
    "timeout": 20,
    "max_retries": 3
  },
  "workflow": {
    "total_timeout": 120,
    "enable_mcp": false,
    "verbose_level": 1,
    "mcp_servers": {
      "fetch-mcp": {
        "command": "node",
        "args": ["{{env.fetch_mcp_script}}"],
        "env": {}
      }
    }
  }
}
